# reload config
cmd + alt + ctrl - r : khd -e "reload"

cmd + ctrl - return : open /Applications/kitty.app
cmd + ctrl - z : fkill amethyst && open /Applications/Amethyst.app
cmd + ctrl - r : open /Applications/RStudio.app

cmd + ctrl - f : open /Applications/Firefox.app

cmd + ctrl - s : open /Applications/Fork.app
cmd + ctrl - d : open /Applications/Dash.app

cmd + ctrl - t : open /Applications/Things3.app
cmd + ctrl - o : open /Applications/Microsoft\ OneNote.app
cmd + ctrl - n : open /Applications/Notability.app
